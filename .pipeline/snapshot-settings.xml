<settings xmlns="http://maven.apache.org/SETTINGS/1.0.0"
   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
   xsi:schemaLocation="http://maven.apache.org/SETTINGS/1.0.0 http://maven.apache.org/xsd/settings-1.0.0.xsd">
   <servers>
      <server>
          <id>artifactory-snapshots</id>
          <username>${env.ARTIFACTORY_USER}</username>
          <password>${env.ARTIFACTORY_TOKEN}</password>
      </server>
   </servers>
   <profiles>
      <profile>
         <id>snapshot.build</id>
         <properties>
            <altSnapshotDeploymentRepository>artifactory-snapshots::https://common.repositories.cloud.sap/artifactory/build-snapshots-cloudsdk</altSnapshotDeploymentRepository>
          </properties>
         <pluginRepositories>
            <pluginRepository>
               <id>artifactory-snapshots</id>
               <url>https://common.repositories.cloud.sap/artifactory/build-snapshots-cloudsdk/</url>
            </pluginRepository>
         </pluginRepositories>
         <repositories>
            <repository>
               <id>artifactory-snapshots</id>
               <url>https://common.repositories.cloud.sap/artifactory/build-snapshots-cloudsdk/</url>
            </repository>
         </repositories>
      </profile>
   </profiles>

   <activeProfiles>
      <activeProfile>snapshot.build</activeProfile>
   </activeProfiles>
</settings>